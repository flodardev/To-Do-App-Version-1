<%- include("header") -%>
        <div class="box" id="heading">
            <h1><%= listTitle %></h1>
        </div>
	
        <div class="box">
            <% for (let i = 0; i < todos.length; i++) { %> 
            <div class="item">
                <input type="checkbox">
                <p>
                    <%=todos[i]%>
                </p>
            </div>
            <% } %>

            <form class="item" method="post">
                <input
                    type="text"
                    name="todo"
                    autocomplete="off"
                    placeholder="Todo..."
                    autofocus
                    required
                />
                <button type="submit" name="list" value=<%= listTitle %>>+</button>
            </form>
        </div>
<%- include("footer") -%>
